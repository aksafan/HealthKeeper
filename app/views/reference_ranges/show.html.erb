<%= render @reference_range %>

<div>
  <% if policy(:reference_range).edit? %>
    <%= link_to "Edit this reference range", edit_reference_range_path(@reference_range) %> |
  <% end %>
  <%= link_to "Back to reference ranges", reference_ranges_path %>

  <% if policy(:reference_range).destroy? %>
    <%= button_to "Destroy this reference range", @reference_range, method: :delete %>
    <% end %>
</div>
